@model AlertMessage

<div class="row">
    <div class="col-md-12">
        <div class="alert alert-@Model.AlertType alert-dismissible fade show" role="alert">
            <div>@Model.Title</div>
            <div>@Model.Message</div>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    </div>
    
</div>
        

